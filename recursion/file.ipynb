   "source": [
    "(4) Factorial"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "054ad135",
   "metadata": {},
   "outputs": [],
   "source": [
    "def factorial(n):\n",
    "    \n",
    "    if (n==0 or n ==1):\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial(n-1)"
   ]
  },
