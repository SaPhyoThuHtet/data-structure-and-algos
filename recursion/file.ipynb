    "def decimal_2_binary(n):\n",
    "    \n",
    "    if (n<2):\n",
    "        print(n)\n",
    "        return \"1\"\n",
    "    else:\n",
    "        return decimal_2_binary(n//2)+str(n%2)\n",
    "    \n",
    "    "
