  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "3074608e",
   "metadata": {},
   "outputs": [],
   "source": [
    "def add_n_natural_numbers(n):\n",
    "    \n",
    "    if (n<=1):\n",
    "        return n\n",
    "    \n",
    "    return n + add_n_natural_numbers(n-1)"
