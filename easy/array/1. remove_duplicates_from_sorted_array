class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        
        if (len(nums) < 2): # if the array length is one, return we don't need to compare it
            return len(nums)
        
        index = 1 
        for i in range(1, len(nums)): # 1, 2, 3 ...
            
            if (nums[i] != nums[i-1]): # Comparing with previous one
                nums[index] = nums[i]
                index = index+1
            
                  
        return index
                
                
                
